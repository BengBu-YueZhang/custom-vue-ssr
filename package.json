{
  "name": "custom-vue-ssr",
  "version": "1.0.0",
  "description": "本项目是基于[vue-hackernews-2.0](https://github.com/vuejs/vue-hackernews-2.0)基础上的自定义修改。将webpack升级到了4.x版本。并提供了Koa2版本的服务端版本。",
  "main": ".eslintrc.js",
  "scripts": {
    "dev": "export PORT=7070 NODE_ENV=development && node server.js",
    "start": "export PORT=7070 NODE_ENV=production && node server.js",
    "build:dev": "rimraf dist && npm run build:client:dev && npm run build:server:dev",
    "build:client:dev": "export NODE_ENV=development && webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server:dev": "export NODE_ENV=development && webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:prod": "rimraf dist && npm run build:client:prod && npm run build:server:prod",
    "build:client:prod": "export NODE_ENV=production && webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server:prod": "export NODE_ENV=production && webpack --config build/webpack.server.config.js --progress --hide-modules",
    "line": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BengBu-YueZhang/custom-vue-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BengBu-YueZhang/custom-vue-ssr/issues"
  },
  "homepage": "https://github.com/BengBu-YueZhang/custom-vue-ssr#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "koa": "^2.7.0",
    "koa-compress": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lru-cache": "^5.1.1",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
